<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ownerIdTxt"
        android:text="이메일 입력" />

    <EditText android:id="@+id/ownerIdEdTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="                    " />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ownerPwTxt"
        android:text="비밀번호 입력" />
    <EditText android:id="@+id/ownerPwEdTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPassword"
        android:text="                   " />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ownerPwReTxt"
        android:text="비밀번호 재입력" />
    <EditText android:id="@+id/ownerPwReEdTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPassword"
        android:text="                   " />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ownerNumTxt"
        android:text="사업자등록번호 입력" />

    <EditText android:id="@+id/ownerNumEdTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="                   "  />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ownerAcntBankTxt"
        android:text="" />

    <Button android:id="@+id/buttonAcntBankBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="은행 선택" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ownerAcntNumTxt"
        android:text="계좌 번호 입력" />
    <EditText android:id="@+id/ownerAcntNumEdTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="                   "  />

    <LinearLayout
        android:id="@+id/con"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
    </LinearLayout>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/add_shop"
        android:text="가계정보 추가"/>



    <TextView
        android:id="@+id/OwnerNumImgName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />
    <Button android:id="@+id/buttonOwnerNum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="샤엽쟈등록번호 선택" />

    <Button android:id="@+id/buttonSubmit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="제출" />

</LinearLayout>